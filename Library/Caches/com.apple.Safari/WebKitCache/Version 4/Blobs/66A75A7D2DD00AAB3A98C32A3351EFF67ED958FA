<div id="answer-29459558" class="answer" data-answerid="29459558"  itemscope itemtype="http://schema.org/Answer">
    <table>
        <tr>
            <td class="votecell">
                

<div class="vote">
        <input type="hidden" name="_id_" value="29459558">
        <a class="vote-up-off" title="This answer is useful">up vote</a>
